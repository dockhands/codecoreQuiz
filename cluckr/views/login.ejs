
<%- include('./partials/header') %>
    
<div class="card m-2"> 
<div class="form-group m-2">

    <form action="/login" method="POST"  class="form-group">
   <label for="">Username</label>
    <input 
    type="text" 
    name="username" 
    class="form-control mr-2"
    placeholder="e.g. Sean Hennesy"
    >

    <label for="">Profile Picture</label>
    <input 
    type="text" 
    name="profile_pic" 
    class="form-control mr-2"
    placeholder="e.g. https://yorkshirefarmhouseeggs.files.wordpress.com/2013/10/foghorn-leghorn.jpg"
    >

    <button class="btn btn-dark mt-2" type="submit">Sign In</button>  
    </form>
</div>

    <%- include('./partials/footer') %>
  

               